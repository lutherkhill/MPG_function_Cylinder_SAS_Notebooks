/*   START OF NODE: One-Way ANOVA   */
%LET _CLIENTTASKLABEL='One-Way ANOVA';
%LET _CLIENTPROCESSFLOWNAME='Process Flow';
%LET _CLIENTPROJECTPATH='';
%LET _CLIENTPROJECTPATHHOST='';
%LET _CLIENTPROJECTNAME='';

GOPTIONS ACCESSIBLE;
/* -------------------------------------------------------------------
   Code generated by SAS Task

   Generated on: Thursday, February 14, 2019 at 5:46:50 PM
   By task: One-Way ANOVA

   Input Data: Local:WORK.MTCARS
   Server:  Local
   ------------------------------------------------------------------- */
ODS GRAPHICS ON;

%_eg_conditional_dropds(WORK.TMP0TempTableInput,
		WORK.TMP1TempTableTemporaryOutput,
		WORK.TMPPlotDS);
/* -------------------------------------------------------------------
   Sort data set Local:WORK.MTCARS
   ------------------------------------------------------------------- */

PROC SQL;
	CREATE VIEW WORK.TMP0TempTableInput AS
		SELECT T.mpg, T.cyl
	FROM WORK.MTCARS as T
;
QUIT;

TITLE;
TITLE1 "One-Way Analysis of Variance";
TITLE2 "Results";

FOOTNOTE;
FOOTNOTE1 "Generated by the SAS System (&_SASSERVERNAME, &SYSSCPL) on %TRIM(%QSYSFUNC(DATE(), NLDATE20.)) at %TRIM(%SYSFUNC(TIME(), TIMEAMPM12.))";

/* -------------------------------------------------------------------
   Run PROC ANOVA to perform the analysis.
   ------------------------------------------------------------------- */
PROC ANOVA DATA=WORK.TMP0TempTableInput ;
	CLASS cyl;
	MODEL mpg = cyl ;
 
	MEANS cyl /  HOVTEST=BARTLETT HOVTEST=BF HOVTEST=LEVENE ;
RUN; QUIT;
/* -------------------------------------------------------------------
   Plots Section
   ------------------------------------------------------------------- */
PROC SORT DATA=WORK.TMP0TempTableInput(KEEP=cyl mpg )
	OUT=WORK.TMPPlotDS;
	BY  cyl;
RUN;


TITLE; 
AXIS1 OFFSET=(10 pct) WIDTH=2; 
AXIS2 MAJOR=(NUMBER=5) WIDTH=2; 
SYMBOL1 I=STD1TJ CI=BLUE CO=BLUE V=NONE H=1 CELLS WIDTH=1; 

PROC GPLOT DATA=WORK.TMPPlotDS ;

TITLE "Means Plot of mpg by cyl";  
	PLOT mpg * cyl / 
		HMINOR=0 VMINOR=0 CAXIS=BLACK 
		DESCRIPTION="Means Plot of mpg by cyl" NAME='MEANS'  
		HAXIS=AXIS1 VAXIS=AXIS2;  
	RUN; 
QUIT; 

AXIS1; AXIS2; SYMBOL1; 
TITLE; 
/* -------------------------------------------------------------------
   End of task code
   ------------------------------------------------------------------- */
RUN; QUIT;
%_eg_conditional_dropds(WORK.TMP0TempTableInput,
		WORK.TMP1TempTableTemporaryOutput,
		WORK.TMPPlotDS);
TITLE; FOOTNOTE;
ODS GRAPHICS OFF;


GOPTIONS NOACCESSIBLE;
%LET _CLIENTTASKLABEL=;
%LET _CLIENTPROCESSFLOWNAME=;
%LET _CLIENTPROJECTPATH=;
